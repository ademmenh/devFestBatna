                    },
                    "number": {
                        "type": "integer",
                        "required": true,
                        "example": 1
                    }
                },
                "position": {
                    "type": "object",
                    "x": {
                        "type": "number",
                        "format": "float",
                        "required": true,
                        "example": 5.0
                    },
                    "y": {
                        "type": "number",
                        "format": "float",
                        "required": true,
                        "example": 5.0
                    }
                },
                "example": [
                        {
                            "data": {
                                "label": "email",
                                "number": 1
                            },
                            "position": {
                                "x": 5.0,
                                "y": 5.0
                            }
                        }
                    ]
            }
        },
        "vectors": {
            "type": "array",
            "items": {
                "prev": {
                    "type": "integer",
                    "required": true,
                    "example": 4
                },
                "next": {
                    "type": "integer",
                    "required": true,
                    "example": 6
