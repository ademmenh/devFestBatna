{
    "type": "object",
    "properties": {
        "userId": {
            "type": "string",
            "example": "4123hl5k32j2153h23k5jh"
        },
        "name": {
            "type": "string",
            "example": "workflow1"
        },
        "description": {
            "type": "string",
            "example": "this workflow automate something."
        },
        "nodes": {
            "type": "array",
            "items": {
                "data": {
                    "type": "object",
